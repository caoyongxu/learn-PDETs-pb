{
  "build": {
    "content": [
      {
        "files": [
          "achievements.yml"
        ],
        "dest": "achievements/main/"
      },
      {
        "src": "pdets",
        "files": [
          "**/*.md",
          "**/*.yml" 
        ],
        "dest": "modules"
      },
      {
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices",
          "configure-network-for-azure-virtual-machines/**",
          "connect-an-app-to-azure-storage/**",
          "control-and-organize-with-azure-resource-manager/**"
        ]
      }
    ],
    "resource": [
      {
        "src": "azure",
        "files": [
          "**/*.gif",
          "**/*.png",
          "**/*.svg",
          "**/*.jpg",
          "**/*.jpeg"
        ],
        "dest": "modules"
      },
      {
        "files": [
          "**/*.gif",
          "**/*.png",
          "**/*.svg",
          "**/*.jpg",
          "**/*.jpeg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**"
        ]
      }
    ],
    "overwrite": [
      
    ],
    "externalReference": [
      
    ],
    "globalMetadata": {
      "breadcrumb_path": "/learn/breadcrumb/toc.json",
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/learn",
      "clicktale": true,
      "uhfHeaderId": "MSDocsHeader-Learn"
    },
    "fileMetadata": {
      "brand": {
        "azure/**/*.yml": "azure"
      },
      "sandbox-disabled": {
        
      }
    },
    "template": [
      
    ],
    "dest": "learn-pr",
    "markdownEngineName": "markdig"
  }
}
